xform version "2.0";

<html><body>
  <h2>My CD Collection</h2>
  <table border={"1"}>
    <tr bgcolor={"#9acd32"}>
      <th style={"text-align:left"}>Title</th>
      <th style={"text-align:left"}>Artist</th>
    </tr>
    {
      for c in /catalog/cd return
        <tr>
          <td>{ c/title/text() }</td>
          <td>{ c/artist/text() }</td>
        </tr>
    }
  </table>
</body></html>
